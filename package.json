{
  "name": "FNS",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "npm run build:contracts && npm run build:frontend",
    "build:frontend": "yarn migrate:deploy && next build",
    "build:contracts": "npm run clean && npm run compile",
    "start": "next start --port ${PORT-3000}",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:deploy": "prisma migrate deploy --preview-feature",
    "migrate:status": "prisma migrate status --preview-feature",
    "generate": "prisma generate",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage",
    "deploy": "npx hardhat run --network goerli scripts/deploy.ts"
  },
  "dependencies": {
    "@prisma/client": "2.15.0",
    "net": "^1.0.2",
    "next": "10.0.6",
    "pg": "^8.5.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "swr": "^0.4.1",
    "tls": "0.0.1",
    "twilio": "^3.60.0"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.1",
    "@nomiclabs/hardhat-etherscan": "^2.1.1",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@prisma/cli": "2.15.0",
    "@typechain/ethers-v5": "^6.0.5",
    "@typechain/hardhat": "^1.0.0",
    "@types/chai": "^4.2.15",
    "@types/chai-as-promised": "^7.1.3",
    "@types/mocha": "^8.2.1",
    "@types/node": "^14.14.32",
    "@types/react": "^17.0.0",
    "chai": "^4.3.3",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^8.2.0",
    "ethereum-waffle": "^3.3.0",
    "ethers": "^5.0.31",
    "hardhat": "^2.1.1",
    "hardhat-typechain": "^0.3.5",
    "ts-generator": "^0.1.1",
    "ts-node": "^9.1.1",
    "typechain": "^4.0.3",
    "typescript": "^4.2.3"
  }
}
